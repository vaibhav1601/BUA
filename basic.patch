+
+public abstract class BasePresenter<V extends BaseView>
+{
+    protected V view;
+    protected Retrofit retrofit;
+
+    protected ProgressDialog progressDialog;
+   
+    protected InputStream inputStream;
+    protected SharedPreferences sharedPreferences;
+
+
+
